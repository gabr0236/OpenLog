<?xml version="1.0" encoding="utf-8"?>
<navigation
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/addLogFragment">

    <fragment
        android:id="@+id/addLogFragment"
        android:name="com.example.openlog.ui.log.AddLogFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_add_log" >
        <action
            android:id="@+id/action_addLogFragment_to_logListFragment"
            app:destination="@id/logListFragment" />
    </fragment>
    <fragment
        android:id="@+id/logListFragment"
        android:name="com.example.openlog.ui.log.LogListFragment"
        android:label="fragment_log_list"
        tools:layout="@layout/fragment_log_list" >
        <action
            android:id="@+id/action_logListFragment_to_addLogFragment"
            app:destination="@id/addLogFragment" />
    </fragment>
</navigation>